{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-console": [
            true,
            "debug",
            "info",
            "time",
            "timeEnd",
            "trace"
        ],
        "arrow-parens": false,
        "semicolon": [
            true,
            "never"
        ],
        "max-line-length": [
            true,
            200
        ],
        "curly": [
            true,
            "ignore-same-line"
        ],
        "ordered-imports": false,
        "quotemark": [
            true,
            "single"
        ],
        "trailing-comma": false,
        "one-variable-per-declaration": false,
        "interface-name": [
            true,
            "always"
        ],
        "object-literal-sort-keys": false,
        "no-var-requires": false,
        "max-classes-per-file": false,
        "member-access": [
            true,
            "no-public"
        ]
    },
    "rulesDirectory": []
}